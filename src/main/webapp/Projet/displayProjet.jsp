            <ul>
                <li class="active"><a href="index.html">Home</a></li>
                <li><a href="showP">Discover</a></li>
                <li><a href="#" onclick="openForm('modalFormProjet')">Add Project</a></li>
                <li><a href="#">Log Out</a></li>
            </ul>
        </nav>
    </aside>
    <main>
        <section class="results">
            <h2>Les Projets</h2>
            <div class="properties">
                <c:forEach items="${showP}" var="projet">
                    <div class="property-card">
                        <img src="https://www.metastrat.com/uploads/content/c-3-raisons-de-lancer-un-projet-a-impact-positif-5e255b8c2e2424f2dc1e3fd4c909e8a1e10bb44a.jpeg" alt="">
                        <h3>${projet.projet_Name}</h3>
                        <p style="overflow: hidden">${projet.projet_Description}</p>
                        <p><strong>Start Date:</strong>${projet.startDate}</p>
                        <p><strong>End Date:</strong> ${projet.endDate}</p>
                        <p><strong>Budget:</strong> ${projet.budget}DH</p>
                        <div class="card-footer">
                            <a href="showT?projet_Id=${projet.getProjet_Id()}" class="btn btn-outline-success">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-eye" viewBox="0 0 16 16">
